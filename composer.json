{
    "name": "karneaud/wp-license-client",
    "description": "License authority SDK for WordPress Plugins for Software License Management",
    "keywords": [
        "client",
        "wordpress"
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "karneaud/wpus-api-client":"dev-main"
    },
    "repositories":[
        {
            "type": "package",
            "package": {
                "name": "karneaud/wppus-api-client",
                "version":"1.0.0",
                "dist": {
                    "url": "https://github.com/karneaud/wppus-api-client/archive/refs/heads/main.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/karneaud/wppus-api-client.git",
                    "type": "git",
                    "reference": "main"
                }
            }
        }
    ],
    "autoload": {
        "classmap": ["./src/class-wp-license-manager.php","./src/wp-license-manager.php"]
    },
    "suggest": {
        "art4/requests-psr18-adapter": "PSR-18 wrapper for Wordpress"
    }
}
